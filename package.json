{
  "name": "duneprototype",
  "productName": "Dune Prototype",
  "version": "0.1.0",
  "description": "Dune Prototype",
  "main": "main.cjs",
  "scripts": {
    "start": "electron ./src/main.cjs",
    "buildcssw": "npx tailwindcss --postcss postcss.config.cjs -c ./tailwind.config.cjs -i ./src/client/templates/styles/main_input.css -o ./src/client/templates/styles/main.css -w",
    "buildcss": "npx tailwindcss --postcss postcss.config.cjs -c ./tailwind.config.cjs -i ./src/client/templates/styles/main_input.css -o ./src/client/templates/styles/main.css",
    "build:test": "set NODE_ENV=production&&electron-builder --dir",
    "build:dist": "set NODE_ENV=production&&electron-builder",
    "build:unpack": "npx asar e a:/games/dune/win-unpacked/resources/app.asar a:/games/dune/win-unpacked/resources/unpacked"
  },
  "author": "oosh",
  "license": "ISC",
  "dependencies": {
    "clipboardy": "^2.2.0",
    "electron-unhandled": "^3.0.2",
    "esm-wallaby": "^3.2.25",
    "handlebars": "^4.7.7",
    "pixi.js": "^6.2.0",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.1",
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.11"
  },
  "build": {
    "appId": "Oosh.Dune.PrePrototype.v030",
    "productName": "Dune",
    "directories": {
      "buildResources": "buildRes",
      "output": "a:/games/Dune"
    },
    "files": [
      {
        "from": "./src/",
        "to": "./",
        "filter": [
          "**/*",
          "!config/*",
          "!mods/"
        ]
      },
      "./node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "./src/mods/",
        "to": "../mods",
        "filter": "**/*"
      },
      {
        "from": "./src/config",
        "to": "../config",
        "filter": "**/default*"
      },
      {
        "from": "./src/saves",
        "to": "../saves",
        "filter": "**/*"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "buildRes/iconAlpha256.png"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "buildRes/iconAlpha256.png",
      "deleteAppDataOnUninstall": true,
      "oneClick": false
    },
    "mac": {
      "category": "Oosh.Dune.Prototype.v010",
      "icon": "buildRes/iconAlpha256.png"
    }
  }
}
